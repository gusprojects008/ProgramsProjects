{
  "System packages": {
    "kalitheme": {
      "feh": "~/.config/feh/themes.conf",
      "kitty": "~/.config/kitty/kitty.conf",
      "picom": "~/.config/picom/picom.conf",
      "i3lock": "~/.config/i3lock/config",
      "scrot": "~/.config/scrot/config",
      "imagemagick": "~/.config/imagemagick/policy.xml",
      "zsh": "~/.zshrc",
      "zsh-syntax-highlighting": "~/.zsh/zsh-syntax-highlighting.zsh",
      "zsh-autosuggestions": "~/.zsh/zsh-autosuggestions.zsh",
