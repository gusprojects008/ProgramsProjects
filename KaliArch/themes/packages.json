      "bash": ["./themes/kalitheme/.bash_logout", "./themes/kalitheme/.bashrc", "./themes/kalitheme/.bashrc.original"],
      "bash-completion": ["./themes/kalitheme/bash.bashrc", "./themes/kalitheme/bash_completion", "./themes/kalitheme/bash_completion.d/"]
